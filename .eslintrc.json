{
    "root": true,
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "extends": ["eslint:recommended", "prettier"],
    "env": {
        "es2021": true,
        "node": true
    },
    "rules": {
        "no-var": "warn",
        "no-unused-vars": "warn",
        "no-undef": "warn"
    },
    "plugins": ["prettier"]
}
